{
  "name": "@yr/weather-symbols",
  "version": "3.2.0",
  "description": "Yr weather symbols",
  "author": "alexander.pope@nrk.no",
  "dependencies": {
    "@yr/component": "3.0.x",
    "@yr/graphics-component": "3.0.x"
  },
  "devDependencies": {
    "buddy": "5.0.x",
    "buddy-plugin-babel": "6.7.x",
    "buddy-plugin-stylus": "*",
    "buddy-plugin-nunjucks": "*",
    "react-dom": "15.0.1",
    "svg-to-png": "2.0.1"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "buddy build",
    "convert": "node bin/convert dist",
    "prepublish": "buddy build && node bin/convert dist",
    "watch": "buddy -rs -g dev watch"
  },
  "repository": "https://github.com/YR/weather-symbols.git",
  "license": "MIT",
  "browser": "index.js",
  "buddy": {
    "build": {
      "targets": [
        {
          "input": "src",
          "output": ".",
          "modular": false
        },
        {
          "input": ["src/preview.js", "src/index.styl", "src/index.nunjs"],
          "output": ".",
          "label": "dev"
        }
      ]
    },
    "server": {
      "port": 3000
    }
  }
}
