{
  "name": "yr-weather-symbols",
  "version": "0.3.0",
  "dependencies": {
    "simple-browser-require": "0.1.7",
    "dustjs-linkedin": "*"
  },
  "devDependencies": {
    "buddy": "~0.17.0"
  },
  "scripts": {
  },
  "main": "index.js",
  "repository": "https://github.com/YR/yr-weather-symbols.git",
  "private": true,
  "buddy": {
    "build": {
      "js": {
        "sources": [
          "src/js"
        ],
        "targets": [
          {
            "input": "src/js/main.js",
            "output": "bin/js/main.js",
            "alias": {
              "dust": "./node_modules/dustjs-linkedin/dist/dust-core-2.0.3.js"
            }
          }
        ]
      },
      "html": {
        "sources": [
          "src/html"
        ],
        "targets": [
          {
            "input": "src/html/index.html",
            "output": "index.html"
          }
        ]
      }
    },
    "settings": {
      "server": {
        "directory": ".",
        "port": 8000
      }
    }
  }
}
