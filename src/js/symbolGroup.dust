{#symbols}
<h2>{title}</h2>
{#variations}
<section class="symbol-group">
	<h3>#{id}</h3>
	<figure class="s128 svg">
		<div class="symbol" data-id="{id}"></div>
		<figcaption>svg@128px</figcaption>
	</figure>
	<figure class="s128 canvas">
		<div class="symbol" data-id="{id}"></div>
		<figcaption>canvas@128px</figcaption>
	</figure>
	<figure class="s128 img">
		<div class="symbol" data-id="{id}"></div>
		<figcaption>img@128px</figcaption>
	</figure>
</section>
{/variations}
{/symbols}
